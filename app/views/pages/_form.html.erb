<br>
<%= form_for @page do |f| %>
<%= content_tag :div, class: 'form-group' do %>
<%= f.label :chapter_id %>
<%= f.select :chapter_id, Chapter.all.collect {|p| [ p.name, p.id ] }, {}, class: 'form-control' %>
<% end %>
<%= content_tag :div, class: 'form-group' do %>
<%= f.label :order %>
<%= f.number_field :order, class: 'form-control' %>
<% end %>
<%= content_tag :div, class: 'form-group' do %>
<%= f.label :name %>
<%= f.text_field :name, class: 'form-control' %>
<% end %>
<%= content_tag :div, class: 'form-group' do %>
<%= f.label :preface %>
<%= f.text_area :preface, class: 'form-control' %>
<% end %>
<%= content_tag :div, class: 'form-group' do %>
<%= f.submit class: 'btn btn-primary' %>
<% end %>
<% end %>
